<?xml version="1.0" encoding="utf-8"?>
<customUI
  xmlns="http://schemas.spaceclaim.com/customui"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<!-- xsi:schemaLocation="http://schemas.spaceclaim.com/customui http://schemas.spaceclaim.com/customui/SpaceClaimCustomUI.V242.xsd"> -->
	<ribbon>
		<tabs>
			<tab id="AESCConstruct2026.Tab" label="AESC Construct">
				<!-- Frame Generator group -->
				<group id="AESCConstruct2026.Group" getLabel="GetRibbonLabel" verticalAlign="top">
					<!--label="Frame Generator"-->
					<button id="AESCConstruct2026.ProfileSidebarBtn"
							command="AESCConstruct2026.ProfileSidebar"
							getLabel="GetRibbonLabel"
							size="large"
							Enabled="False"/>
				</group>

				<group id="AESCConstruct2026.ToolsGroup" getLabel="GetRibbonLabel">
					<container id="AESC.Construct.SketchToolGroupContainer"
					   layoutOrientation="horizontal"
					   isGroup="false">
						
						<container id="AESC.Construct.sketchToolsContainer1"
							layoutOrientation="vertical"
							isGroup="true"
							itemSpacing="0">
							<button id="AESC.Construct.Line"
									command="Line"/>
							<button id="AESC.Construct.Rect"
									command="Rect"/>
							<button id="AESC.Construct.CurveOffset"
									command="CurveOffset"/>
						</container>

						<container id="AESC.Construct.sketchToolsContainer2"
						   layoutOrientation="vertical"
						   isGroup="true"
						   itemSpacing="0">
							<button id="AESC.Construct.SetModeSketch"
									command="SetModeSketch"
									size="small"
									showLabel="false"
									showImage="true"/>
							<button id="AESC.Construct.SetModeSection"
									command="SetModeSection"
									size="small"
									showLabel="false"
									showImage="true"/>
							<button id="AESC.Construct.SetMode3D"
									command="AESC.Construct.SetMode3D"
									size="small"
									showLabel="false"
									showImage="true"/>
						</container>
					</container>

					<separator id="AESC.Construct.separator2"/>

					<button id="AESC.Construct.CopyEdges"
							command="CopyEdges"
							size="large"
							showImage="true"
							showLabel="false"
							style="imageOnly"/>
				</group>

				<group id="AESCConstruct2026.ExportGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.ExportSTEPBtn"
							command="AESCConstruct2026.ExportSTEP"
							getLabel="GetRibbonLabel"
							size="large"
							style="imageandtext"/>
					<button id="AESCConstruct2026.CompareBodiesBtn"
							  command="AESCConstruct2026.CompareBodies"
							  getLabel="GetRibbonLabel"
							  size="large"
							  style="imageandtext"/>
					<button id="AESCConstruct2026.DetectClashesBtn"
							  command="AESCConstruct2026.DetectClashes"
							  getLabel="GetRibbonLabel"
							  size="large"
							  style="imageandtext"/>
					<button id="AESCConstruct2026.ExportBOMBtn"
							command="AESCConstruct2026.ExportBOM"
							getLabel="GetRibbonLabel"
							size="large"
							style="imageandtext"/>
					<button style=""
							id="AESCConstruct2026.BOMMenu">
						<button id="AESCConstruct2026.UpdateBOM"
								command="AESCConstruct2026.UpdateBOM"
								getLabel="GetRibbonLabel"
								size="large"
								showLabel="true"/>
						<button id="AESCConstruct2026.ExportExcelBtn"
								command="AESCConstruct2026.ExportExcel"
								getLabel="GetRibbonLabel"
								size="large"
								style="imageandtext"/>
					</button>
				</group>

				<group id="AESCConstruct2026.PlateGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.Plate"
							command="AESCConstruct2026.Plate"
							getLabel="GetRibbonLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.FastenerGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.Fastener"
							command="AESCConstruct2026.Fastener"
							getLabel="GetRibbonLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.ConnectorGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.ConnectorSidebarBtn"
							command="AESCConstruct2026.ConnectorSidebar"
							getLabel="GetRibbonLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.RibCutOutGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.RibCutOut"
							command="AESCConstruct2026.RibCutOut"
							getLabel="GetRibbonLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.Engraving" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.EngravingBtn"
							command="AESCConstruct2026.EngravingControl"
							getLabel="GetEngravingLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.CustomProperties" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.CustomPropertiesBtn"
							command="AESCConstruct2026.CustomProperties"
							getLabel="GetCustomPropertiesLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.ToolGroup" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.SettingsSidebarBtn"
							command="AESCConstruct2026.SettingsSidebar"
							 getLabel="GetRibbonLabel"
							size="large"/>
				</group>

				<group id="AESCConstruct2026.Network" getLabel="GetRibbonLabel">
					<button id="AESCConstruct2026.ActivateNetworkBtn"
						command="AESCConstruct2026.ActivateNetwork"
						getLabel="GetRibbonLabel"
						size="large"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
